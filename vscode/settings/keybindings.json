[
  // -------------------------------
  // General
  // -------------------------------
  {
    "key": "shift+cmd+c",
    "command": "-workbench.action.terminal.openNativeConsole",
    "when": "!terminalFocus"
  },
  {
    "key": "shift+cmd+c",
    "command": "copyRelativeFilePath"
  },
  {
    "key": "shift+cmd+alt+c",
    "command": "copy-relative-path-and-line-numbers.both"
  },
  // -------------------------------
  // Activity Bar
  // -------------------------------
  // -------------------------------
  // Sidebar
  // -------------------------------
  // -------------------------------
  // Editor
  // -------------------------------
  // Cursor Control -------------------------------
  // previousEditor
  {
    "key": "ctrl+g p",
    "command": "workbench.action.previousEditor"
  },
  {
    "key": "alt+cmd+left",
    "command": "-workbench.action.previousEditor"
  },
  // nextEditor
  {
    "key": "ctrl+g n",
    "command": "workbench.action.nextEditor"
  },
  {
    "key": "alt+cmd+right",
    "command": "-workbench.action.nextEditor"
  },
  // focusNextGroup
  {
    "key": "ctrl+g ctrl+n",
    "command": "workbench.action.focusNextGroup"
  },
  {
    "key": "ctrl+w ctrl+w",
    "command": "-workbench.action.focusNextGroup",
    "when": "!editorTextFocus && !filesExplorerFocus && !inSearchEditor && !replaceInputBoxFocus && !searchViewletFocus && !terminalFocus"
  },
  // focusPreviousGroup
  {
    "key": "ctrl+g ctrl+p",
    "command": "workbench.action.focusPreviousGroup"
  },
  // Pane Control -------------------------------
  // 【 moveActiveEditorGroup 】
  // moveActiveEditorGroupDown
  {
    "key": "ctrl+g j",
    "command": "workbench.action.moveActiveEditorGroupDown"
  },
  {
    "key": "cmd+k down",
    "command": "-workbench.action.moveActiveEditorGroupDown"
  },
  // moveActiveEditorGroupLeft
  {
    "key": "ctrl+g h",
    "command": "workbench.action.moveActiveEditorGroupLeft"
  },
  {
    "key": "cmd+k left",
    "command": "-workbench.action.moveActiveEditorGroupLeft"
  },
  // moveActiveEditorGroupRight
  {
    "key": "ctrl+g l",
    "command": "workbench.action.moveActiveEditorGroupRight"
  },
  {
    "key": "cmd+k right",
    "command": "-workbench.action.moveActiveEditorGroupRight"
  },
  // moveActiveEditorGroupUp
  {
    "key": "ctrl+g k",
    "command": "workbench.action.moveActiveEditorGroupUp"
  },
  {
    "key": "cmd+k up",
    "command": "-workbench.action.moveActiveEditorGroupUp"
  },
  // 【 moveEditorToGroup 】
  // moveEditorToPreviousGroup
  {
    "key": "ctrl+g ctrl+h",
    "command": "workbench.action.moveEditorToPreviousGroup",
    "when": "editorTextFocus && !terminalFocus"
  },
  {
    "key": "ctrl+cmd+right",
    "command": "-workbench.action.moveEditorToPreviousGroup",
    "when": "editorTextFocus && !terminalFocus"
  },
  // moveEditorToNextGroup
  {
    "key": "ctrl+g ctrl+l",
    "command": "workbench.action.moveEditorToNextGroup",
    "when": "editorTextFocus && !terminalFocus"
  },
  {
    "key": "ctrl+cmd+right",
    "command": "-workbench.action.moveEditorToNextGroup",
    "when": "editorTextFocus && !terminalFocus"
  },
  // 【 moveEditorInGroup 】
  // moveEditorLeftInGroup
  {
    "key": "ctrl+g shift+h",
    "command": "workbench.action.moveEditorLeftInGroup",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+k shift+cmd+left",
    "command": "-workbench.action.moveEditorLeftInGroup",
    "when": "editorTextFocus"
  },
  // moveEditorRightInGroup
  {
    "key": "ctrl+g ctrl+l",
    "command": "workbench.action.moveEditorRightInGroup",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+k shift+cmd+right",
    "command": "-workbench.action.moveEditorRightInGroup",
    "when": "editorTextFocus"
  },
  // 【 splitEditor 】
  // splitEditorDown
  {
    "key": "ctrl+g -",
    "command": "workbench.action.splitEditorDown"
  },
  {
    "key": "cmd+k cmd+\\",
    "command": "-workbench.action.splitEditorDown"
  },
  // splitEditorOrthogonal
  {
    "key": "ctrl+g cmd+-",
    "command": "workbench.action.splitEditorOrthogonal"
  },
  {
    "key": "cmd+k cmd+\\",
    "command": "-workbench.action.splitEditorOrthogonal"
  },
  // splitEditorRight
  {
    "key": "ctrl+g \\",
    "command": "workbench.action.splitEditorRight"
  },
  {
    "key": "cmd+k cmd+\\",
    "command": "-workbench.action.splitEditorRight"
  },
  // splitEditorLeft
  {
    "key": "ctrl+g shift+\\",
    "command": "workbench.action.splitEditorLeft"
  },
  {
    "key": "cmd+k cmd+\\",
    "command": "-workbench.action.splitEditorLeft"
  },
  // splitEditorInGroup
  {
    "key": "ctrl+g ctrl+alt+cmd+\\",
    "command": "workbench.action.splitEditorInGroup",
    "when": "activeEditorCanSplitInGroup"
  },
  {
    "key": "cmd+k shift+cmd+\\",
    "command": "-workbench.action.splitEditorInGroup",
    "when": "activeEditorCanSplitInGroup"
  },
  // -------------------------------
  // Indentation
  // -------------------------------
  // disable default indent/outdent to avoid conflict with backward/forward history
  {
    "key": "cmd+]",
    "command": "-editor.action.indentLines",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "cmd+[",
    "command": "-editor.action.outdentLines",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "cmd+[",
    "command": "-markdown.extension.onOutdentLines",
    "when": "editorTextFocus && !suggestWidgetVisible && editorLangId =~ /^markdown$|^rmd$|^quarto$/"
  },
  {
    "key": "cmd+]",
    "command": "-markdown.extension.onIndentLines",
    "when": "editorTextFocus && !suggestWidgetVisible && editorLangId =~ /^markdown$|^rmd$|^quarto$/"
  },
  // -------------------------------
  // Panel
  // -------------------------------
  // [next/previous]PainView
  // ※ replace default indentation settings
  {
    "key": "cmd+]",
    "command": "workbench.action.nextPanelView"
  },
  {
    "key": "cmd+[",
    "command": "workbench.action.previousPanelView"
  },
  // 【 Terminal 】
  // focusNextPane
  {
    "key": "ctrl+g o",
    "command": "workbench.action.terminal.focusNextPane",
    "when": "terminalFocus && terminalHasBeenCreated || terminalFocus && terminalProcessSupported"
  },
  {
    "key": "alt+cmd+right",
    "command": "-workbench.action.terminal.focusNextPane",
    "when": "terminalFocus && terminalHasBeenCreated || terminalFocus && terminalProcessSupported"
  },
  // focusPreviousPane
  {
    "key": "ctrl+g ;",
    "command": "workbench.action.terminal.focusPreviousPane",
    "when": "terminalFocus && terminalHasBeenCreated || terminalFocus && terminalProcessSupported"
  },
  {
    "key": "alt+cmd+left",
    "command": "-workbench.action.terminal.focusPreviousPane",
    "when": "terminalFocus && terminalHasBeenCreated || terminalFocus && terminalProcessSupported"
  },
  // splitInActiveWorkspace
  {
    "key": "ctrl+g \\",
    "command": "workbench.action.terminal.splitInActiveWorkspace",
    "when": "terminalFocus && terminalProcessSupported || terminalFocus && terminalWebExtensionContributedProfile"
  },
  {
    "key": "shift+cmd+]",
    "command": "-workbench.action.nextEditor"
  },
  {
    "key": "cmd+`",
    "command": "workbench.action.terminal.toggleTerminal",
    "when": "terminal.active"
  },
  {
    "key": "ctrl+`",
    "command": "-workbench.action.terminal.toggleTerminal",
    "when": "terminal.active"
  },
  {
    "key": "shift+alt+cmd+`",
    "command": "workbench.actions.view.problems",
    "when": "workbench.panel.markers.view.active"
  },
  {
    "key": "shift+cmd+m",
    "command": "-workbench.actions.view.problems",
    "when": "workbench.panel.markers.view.active"
  },
  {
    "key": "cmd+[",
    "command": "workbench.action.navigateBack",
    "when": "canNavigateBack"
  },
  {
    "key": "cmd+]",
    "command": "workbench.action.navigateForward",
    "when": "canNavigateBack"
  }
  // -------------------------------
  // Status Bar
  // -------------------------------
  // -------------------------------
  // Extensions
  // -------------------------------
  // -------------------------------
  // Inbox
  // -------------------------------
]
